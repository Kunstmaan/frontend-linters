{
    "extends": [
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "ter-indent": [
            true,
            4
        ],
        "no-unused-variable": true,
        "no-unused-expression": true,
        "function-name": false
    }
}
